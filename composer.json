        "yiisoft/yii2-elasticsearch": "~2.0.0",
        "omnilight/yii2-scheduling": "^1.0",
        "php-curl-class/php-curl-class": "^8.3",
        "phpoffice/phpspreadsheet": "~1.5",
        "guzzlehttp/guzzle": "^6.2",
        "jpush/jpush": "v3.6.*",
        "workerman/workerman": "^3.5",
        "conquer/oauth2": "*",
